{
  "name": "web",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "dev": "next dev",
    "build": "node scripts/build.mjs",
    "ds:audit": "node scripts/design-system-audit.mjs",
    "vercel-build": "node scripts/build.mjs",
    "start": "next start",
    "test:mobile": "playwright test",
    "test:mobile:local": "playwright test",
    "test:mobile:neon": "node scripts/assert-env.mjs && playwright test",
    "test:mobile:neon:allowprod": "cross-env ALLOW_PROD_TEST_DB=YES node scripts/assert-env.mjs && playwright test",
    "test:ai-plan-builder": "node scripts/test-ai-plan-builder.mjs",
    "test:ai-plan-builder:parallel": "node scripts/test-ai-plan-builder.mjs --pwShards=4 --pwRepeatOn=2",
    "postinstall": "npx prisma generate --schema prisma/schema.prisma && node scripts/ensure-prisma-client-proxy.mjs",
    "prisma:generate": "npx prisma generate --schema prisma/schema.prisma && node scripts/ensure-prisma-client-proxy.mjs",
    "prisma:migrate:deploy": "npx prisma migrate deploy --schema prisma/schema.prisma",
    "prisma:migrate:init": "prisma migrate dev --name init --create-only",
    "seed": "ts-node --project tsconfig.prisma.json prisma/seed.ts"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "dependencies": {
    "@clerk/nextjs": "^6.36.7",
    "@prisma/client": "^6.19.1",
    "@vercel/blob": "^0.27.0",
    "next": "^14.2.12",
    "papaparse": "^5.5.3",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@playwright/test": "^1.57.0",
    "@types/node": "^20.12.12",
    "@types/papaparse": "^5.5.2",
    "@types/react": "^18.3.7",
    "@types/react-dom": "^18.3.2",
    "autoprefixer": "^10.4.23",
    "cross-env": "^7.0.3",
    "playwright": "^1.57.0",
    "postcss": "^8.5.6",
    "prisma": "^6.19.1",
    "tailwindcss": "^3.4.13",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.3",
    "vitest": "^2.1.9"
  }
}
